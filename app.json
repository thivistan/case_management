{
  "expo": {
    "name": "CaseManagementApp",
    "slug": "CaseManagementApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/thaddeus_globe.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/thaddeus_globe.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to find housing, food, and health services."
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/thaddeus_globe.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}